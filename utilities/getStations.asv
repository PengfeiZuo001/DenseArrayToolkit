function stationList = getStations(DataStruct)
stationList_all = {};
for n = 1:length(DataStruct)
    stationList_all(n).code = DataStruct(n).StationInfo.sta;
    stationList(n).stla = DataStruct(n).StationInfo.stla;
    stationList(n).stlo = DataStruct(n).StationInfo.stlo;

end
code = unique({stationList.code})';
stla = unique({stationList.stla})';
stlo = unique({stationList.stlo})';


stationList.code = code;
stationList.stla = stla;
stationList.stlo = stlo;
